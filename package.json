{
  "name": "fivem-ts-boilerplate",
  "version": "0.0.1",
  "description": "Typescript Boilerplate for FiveM",
  "main": "index.js",
  "repository": "https://github.com/otfgaming/fivem-typescript-boilerplate",
  "author": "Lockhinator <lockhinator@users.noreply.github.com>",
  "scripts": {
    "build:dev": "webpack --mode production",
    "build": "webpack --mode production",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx src",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore ./src",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "watch": "webpack --mode development --watch true",
    "test": "jest"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@citizenfx/client": "^2.0.5755-1",
    "@citizenfx/server": "^2.0.5755-1",
    "@types/node": "^16.0"
  },
  "devDependencies": {
    "@types/jest": "^28.0.7",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
